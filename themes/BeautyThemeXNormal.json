  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#BBBBBB4D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation comments",
      "scope": ["comment.block.documentation", "comment.line.documentation"],
      "settings": {
        "foreground": "#BBBBBB66",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.language"],
      "settings": {
        "foreground": "#F09483E6"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#F09483E6"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#F09483E6"
      }
    },
    {
      "name": "Null/Undefined",
      "scope": [
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.nan"
      ],
      "settings": {
        "foreground": "#F09483E6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "Function names",
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function.any-method"
      ],
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Method names",
      "scope": [
        "entity.name.function.member",
        "meta.method-call",
        "meta.method.declaration"
      ],
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "Static types",
      "scope": [
        "entity.name.type",
        "storage.type.cs",
        "storage.type.class",
        "storage.type.interface",
        "storage.type.enum"
      ],
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.type.class",
        "entity.name.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "Interface names",
      "scope": ["entity.name.type.interface", "entity.name.interface"],
      "settings": {
        "foreground": "#FAC29AE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type parameters (Generics)",
      "scope": [
        "entity.name.type.parameter",
        "meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#FAC29AE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag attributes",
      "scope": ["entity.other.attribute-name", "meta.attribute"],
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "CSS Class selectors",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.object.property",
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Language variables (this, self, super)",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#FAC29AE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#E95678E6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Control keywords",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow"
      ],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Import/Export keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete",
        "keyword.operator.typeof",
        "keyword.operator.instanceof"
      ],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Arithmetic operators",
      "scope": ["keyword.operator.arithmetic", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Arrow functions",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "Storage keywords",
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Storage modifiers (async, static, etc)",
      "scope": [
        "storage.modifier.async",
        "storage.modifier.static",
        "storage.modifier.abstract"
      ],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Strings",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#DB2777"
      }
    },
    {
      "name": "Regex operators",
      "scope": [
        "keyword.operator.quantifier.regexp",
        "keyword.operator.or.regexp"
      ],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "Provided functions",
      "scope": ["support.function", "support.function.builtin"],
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Built-in types",
      "scope": ["support.type", "support.class", "support.type.primitive"],
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "Provided variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "Console/Debug",
      "scope": ["support.type.object.console", "entity.name.function.console"],
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": [
        "support.type.property-name",
        "meta.object-literal.key",
        "string.unquoted.label"
      ],
      "settings": {
        "foreground": "#DB2777E6"
      }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "CSS property values",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "Decorators/Annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Tag brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Key/value separators",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Component Tags",
      "scope": [
        "support.class.component.astro",
        "support.class.component.js",
        "support.class.component.ts",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "entity.name.type.class.tsx",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx",
        "meta.tag.other"
      ],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "JSX/TSX Props",
      "scope": [
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#25B0BCE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#B877DBE6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#B877DBE6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#B877DBE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#ABB2BF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Code inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Markdown - Code block",
      "scope": "markup.fenced_code.block",
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Markdown - Links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "Markdown - Link descriptions",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Markdown - List punctuation",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "JSON - Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json"
      ],
      "settings": {
        "foreground": "#DB2777E6"
      }
    },
    {
      "name": "YAML - Keys",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#DB2777E6"
      }
    },
    {
      "name": "Invalid/Deprecated",
      "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#E95678E6",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Diffs - Inserted",
      "scope": ["markup.inserted", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#09F7A0"
      }
    },
    {
      "name": "Diffs - Deleted",
      "scope": ["markup.deleted", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "Diffs - Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#FAB795E6"
      }
    }
  ]
}
